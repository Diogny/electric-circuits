{
	"name": "electric-circuits",
	"author": "Diogny A. B. <diognyab@outlook.com>",
	"version": "1.1.1",
	"description": "Creates an electric circuits app",
	"license": "ISC",
	"main": "./dist/index.js",
	"homepage": "https://github.com/Diogny/electric-circuits",
	"bugs": {
		"url": "https://github.com/Diogny/electric-circuits/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Diogny/electric-circuits.git"
	},
	"keywords": [
		"Electron",
		"electric",
		"circuits",
		"units",
		"svg",
		"package",
		"dabberio"
	],
	"scripts": {
		"no-test": "echo \"Error: no test specified\" && exit 1",
		"test": "mocha --reporter spec",
		"preinstall": "node -e \"!process.env.npm_config_user_agent.startsWith('pnpm/') && !console.log('Use \\`npx pnpm install\\` to install dependencies in this repository\\n') && process.exit(1)\"",
		"build-ts": "tsc",
		"watch": "tsc -w",
		"eslint": "eslint \"./src/*.ts\" \"./src/**/*.ts\" --report-unused-disable-directives",
		"build-dev": "pnpm run build-ts && webpack --config webpack.dev.js",
		"start": "pnpm run build-dev && pnpm run app",
		"app": "electron ./dist/index.js",
		"webpack-watch": "webpack --watch",
		"serve": "npm run build:main && electron ./dist/index.js --serve",
		"build:main": "tsc && webpack --config webpack.dev.js"
	},
	"files": [],
	"devDependencies": {
		"@types/node": "12.12.21",
		"chai": "^4.2.0",
		"electron": "^9.0.2",
		"electron-reload": "^1.5.0",
		"eslint": "^7.2.0",
		"html-webpack-plugin": "^4.3.0",
		"mocha": "^7.2.0",
		"ts-loader": "^7.0.5",
		"tsconfig-paths-webpack-plugin": "^3.2.0",
		"typescript": "^3.9.5",
		"webpack": "^4.43.0",
		"webpack-bundle-analyzer": "^3.8.0",
		"webpack-cli": "^3.3.11"
	}
}
